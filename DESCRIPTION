Package: dosearch
Type: Package
Title: Causal Effect Identification from Multiple Incomplete Data Sources
Version: 1.0.9
Authors@R: c(
    person(given = "Santtu",
           family = "Tikka",
           role = c("aut", "cre"),
           email = "santtuth@gmail.com",
